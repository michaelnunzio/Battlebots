<div class="row">
    <div class="col s12">
        <table>
            <thead>
                <tr>
                    <th>Part Name</th>
                    <th>Attack</th>
                    <th>Armor</th>
                    <th>Available</th>
                    <th>---</th>
                </tr>
            </thead>
            <tbody>
                {{#each available_parts}}
                <tr>
                    <td>{{name}}</td>
                    <td>{{damage}}</td>
                    <td>{{armor}}</td>
                    <td>{{available}}</td>
                    <td><a class="btn waves-effect waves-light pick-part" data-part-id="{{part_id}}" data-robot-id="{{robot_id}}" data-position-id="{{position_id}}">Pick</a></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>