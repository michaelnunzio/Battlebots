<div class="row">
    <div class="col s12">
        <table>
            <thead>
                <tr>
                    <th>Part Name</th>
                    <th>Attack</th>
                    <th>Armor</th>
                    <th>Available</th>
                    <th>---</th>
                </tr>
            </thead>
            <tbody>
                {{#each response.available_parts}}
                <tr>
                    <td>{{name}}</td>
                    <td>{{damage}}</td>
                    <td>{{armor}}</td>
                    <td>{{available}}</td>
                    {{#if partExists}}
                    <td><a class="btn waves-effect waves-light replace-part" data-part-id="{{part_id}}" data-robot-id="{{robot_id}}" data-position-id="{{position_id}}">Replace</a></td>
                    {{else}}
                    <td><a class="btn waves-effect waves-light add-part" data-user-id={{user_id}} data-part-id="{{part_id}}" data-robot-id="{{robot_id}}" data-position-id="{{position_id}}">Add</a></td>
                    {{/if}}
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>